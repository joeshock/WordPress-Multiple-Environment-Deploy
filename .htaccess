# BEGIN WordPress

<IfModule mod_rewrite.c>

RewriteEngine On

# Uncomment if environment is utilizing root directory or subdomain
RewriteBase /

# Uncomment if environment is utilizing one subdirectory
#RewriteBase /projectname/

# Uncomment if environment is utilizing two subdirectories
#RewriteBase /clientname/projectname/

RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Uncomment if environment is utilizing root directory or subdomain
RewriteRule . /index.php [L]

# Uncomment if environment is utilizing one subdirectory
#RewriteRule . /projectname/index.php [L]

# Uncomment if environment is utilizing two subdirectories
#RewriteRule . /clientname/projectname/index.php [L]


</IfModule>

# END WordPress